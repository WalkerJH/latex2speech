<?xml version="1.0" encoding="UTF-8"?>

<ssmlfuncs>

    <!-- User Defined -->
    <userFunction>
        <funcname />
        <text>of</text>
        <repeat />
        <arg />
        <text>,</text>
    </userFunction>
    
    <!-- Equality -->
    <Eq>
        <arg />
        <text>equals</text>
        <arg />
    </Eq>
    
    <Ne>
        <arg />
        <text>not equal to</text>
        <arg />
    </Ne>

    <Lt>
        <arg />
        <text>less than</text>
        <arg />
    </Lt>

    <Le>
        <arg />
        <text>less than or equal to</text>
        <arg />
    </Le>

    <Gt>
        <arg />
        <text>greater than</text>
        <arg />
    </Gt>

    <Ge>
        <arg />
        <text>greater than or equal to</text>
        <arg />
    </Ge>

    <!-- Basic Operators -->
    <Add>
        <arg />
        <repeat />
        <text>plus</text>
        <arg />
    </Add>

    <Mul>
        <arg />
        <repeat />
        <text>times</text>
        <arg />
    </Mul>

    <Pow>
        <arg />
        <text>to the power of</text>
        <arg />
    </Pow>
    <!-- Notation -->

    <!-- Trigonometry -->
    <sin>
        <text>sin of</text>
        <arg />
    </sin>

    <cos>
        <text>cosine of</text>
        <arg />
    </cos>

    <tan>
        <text>tangent of</text>
        <arg />
    </tan>

    <cot>
        <text>cotangent of</text>
        <arg />
    </cot>

    <sec>
        <text>secant of</text>
        <arg />
    </sec>

    <csc>
        <text>cosecant of</text>
        <arg />
    </csc>

    <asin>
        <text>arc sin of</text>
        <arg />
    </asin>

    <acos>
        <text>arc cosine of</text>
        <arg />
    </acos>

    <atan>
        <text>arc tangent of</text>
        <arg />
    </atan>

    <acot>
        <text>arc cotangent of</text>
        <arg />
    </acot>

    <asec>
        <text>arc secant of</text>
        <arg />
    </asec>

    <acsc>
        <text>arc cosecant of</text>
        <arg />
    </acsc>

    <!-- Numerical -->
    <ceiling>
        <text>ceiling of</text>
        <arg />
    </ceiling>

    <floor>
        <text>floor of</text>
        <arg />
    </floor>

    <sign>
        <text>sign of</text>
        <arg />
    </sign>

    <Abs>
        <text>absolute value of</text>
        <arg />
    </Abs>

    <Min>
        <text>minuimum value among</text>
        <repeat />
        <arg />
        <text>,</text>
    </Min>
 
    <Max>
        <text>maximum value among</text>
        <repeat />
        <arg />
        <text>,</text>
    </Max>

    <!-- Imaginary Numbers -->
    <im>
        <text>imaginary part of</text>
        <arg />
    </im>

    <re>
        <text>real part of</text>
        <arg />
    </re>

    <conjugate>
        <text>conjugate of</text>
        <arg />
    </conjugate>

    <!-- Summation and Series-->
    <summation>
        <text>sum of</text>
        <arg />
        <text>from</text>
        <arg />
        <text>equals</text>
        <arg />
        <text>to</text>
        <arg />
    </summation>

    <!-- Calculus-->
    <Derivative>
        <text>derivative of</text>
        <arg />
        <text>with respect to</text>
        <arg />
    </Derivative>
    
    <Integral>
        <text>integral of</text>
        <arg />
        <text>d</text>
        <arg />
    </Integral>

</ssmlfuncs>
    
