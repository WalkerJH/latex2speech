<?xml version="1.0" encoding="UTF-8"?>

<mathmode>

    <!-- Valid Raw Unicode Math Symbols -->
    <symb name="=">
        <say_as>equals</say_as>
    </symb>
    <symb name="&lt;">
        <say_as>less than</say_as>
    </symb>
    <symb name="&gt;">
        <say_as>greater than</say_as>
    </symb>
    <symb name="+">
