    </symb>
    <symb name="-">
        <say_as>minus</say_as>
    </symb>
    <symb name="/">
        <say_as>divided by</say_as>
    </symb>
     <symb name="!">
        <say_as>factorial</say_as>
    </symb>
    
    <!-- Relation Commands -->
    <cmd name="leq">
        <say_as>less than or equal to</say_as>
    </cmd>
    <cmd name="geq">
        <say_as>greater than or equal to</say_as>
    </cmd>
    <cmd name="times">
