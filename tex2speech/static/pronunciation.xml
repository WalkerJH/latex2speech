<?xml version="1.0" encoding="UTF-8"?>

<latex>
<!-- LaTeX Commands -->
    <cmd name = "title" type = "none">
        Title 
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.3s"/>
    </cmd>

    <cmd name = "author" type = "none">
        By:
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.3s"/>
    </cmd>

    <cmd name = "date" type = "none">
        Published:
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.3s"/>
    </cmd>

    <cmd name="section" type = "none">
        Section: 
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.3s"/>
    </cmd>

    <cmd name="subsection" type = "none">
        Sub Section: 
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.3s"/>
    </cmd>

    <cmd name="dots" type = "none">
        <break strength="strong"/>
    </cmd>

    <cmd name="em" type = "none">
        <emphasis level = "strong">
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name="emph" type = "none">
        <emphasis level = "strong">
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name="textit" type = "none">
        <emphasis level = "strong">
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name="textbf" type = "none">
        <emphasis level = "strong">
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name = "texttt" type = "none">
        <emphasis level = "strong">
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name = "cite" type = "none">
        <emphasis level = "reduced">
            Cited in reference as:
            <break time = "0.3s"/>
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name = "bibitem" type = "none">
        <break time = "1s"/>
        Bibliography item is read as:
        <break time = "0.5s"/>
        <arg num = "1"/>
    </cmd>

    <cmd name = "LaTeX" type = "none">
        LaTeX
    </cmd>

    <cmd name = "caption" type = "none">
        Caption:
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.5s"/>
    </cmd>

    <!-- This is incase user does \thebibliography -->
    <cmd name = "thebibliography" type = "none">
        <emphasis level = "strong">
            Reference Section: 
        </emphasis>
    </cmd>

    <cmd name = "footnote" type = "none">
        Footnote:
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.5s"/>
    </cmd>

    <cmd name = "TeX" type = "none">
        TeX
    </cmd>

<!-- Latex Environments -->
    <!-- This is incase user does \begin{thebibliography} -->
    <env name = "thebibliography" type = "none">
        <says>
            <emphasis level = "strong">
                Reference Section: 
            </emphasis>
                <content/>
        </says>
    </env>

    <env name="equation" type="mathmode">
        <says>
            the equation
            <content/>
        </says>
    </env>

    <env name="eqnarray" type="mathmode">
        <says>
            <content/>
        </says>
    </env>

    <env name="$" type="mathmode">
        <says>
            <content/>
        </says>
    </env>

    <env name="$$" type="mathmode">
        <says>
            <content/>
        </says>
    </env>

    <env name="displaymath" type="mathmode"> <!-- Also alias for \[...\] in TexSoup -->
        <says>
            <content/>
        </says>
    </env>

    <env name="math" type="mathmode"> <!-- Also alias for \(...\) in TexSoup-->
        <says>
            <content/>
        </says>
    </env>

    <env name="itemize" type = "none">
        <says>
            <content/>
        </says>

        <!-- This might be wrong, because we want the contents to be read with break time infront? -->
        <defines>
            <cmd name="item" type = "none">
                <break time = "40ms"/>
            </cmd>

        </defines>

        <break time = "medium"/>
    </env>

    <env name="quotation" type = "none">
        <says>
            <p>
                <content/>
            </p>
        </says>
    </env>

    <env name="quote" type = "none">
        <says>
            <p>
                <content/>
            </p>
        </says>
    </env>

    <env name="center" type = "none">
        <says>
            <p>
                <content/>
            </p>
        </says>
    </env>

    <env name="flushleft" type = "none">
        <says>
            <p>
                <content/>
            </p>
        </says>
    </env>

    <env name="flushright" type = "none">
        <says>
            <p>
                <content/>
            </p>
        </says>
    </env>

    <env name="verse" type = "none">
        <says>
            <p>
                <content/>
            </p>
        </says>
    </env>

    <env name="enumerate" type = "none">
    </env>

    <env name="description" type = "none">
    </env>

    <env name="list" type = "none">
    </env>

    <env name="array" type = "none">
    </env>

    <env name="table" type = "none">
        <says>
            Begin Table:
            <break time = "0.3s"/>
            <content/>
            End Table
            <break time = "0.3s"/>
        </says>
    </env>

    <env name="tabular" type = "readTable">
        <says>
            Table Contents:
            <break time = "40ms"/>
            <content/>
            End Table Contents
            <break time = "40ms"/>
        </says>
    </env>

    <env name="abstract" type = "none">
    </env>

    <env name="minipage" type = "none">
    </env>

    <env name="split" type = "none">
    </env>

    <env name="tabbing" type = "none">
    </env>

    <env name="titlepage" type = "none">
    </env>

    <env name="figure" type = "none">
    </env>

    <env name="picture" type = "none">
    </env>

    <env name="verbatim" type = "none">
    </env>
</latex>
