<?xml version="1.0" encoding="UTF-8"?>

<latex>
<!-- LaTeX Commands -->
    <cmd name = "title">
        Title 
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.3s"/>
    </cmd>

    <cmd name = "author">
        By:
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.3s"/>
    </cmd>

    <cmd name = "date">
        Published:
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.3s"/>
    </cmd>

    <cmd name="section">
        Section: 
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.3s"/>
    </cmd>

    <cmd name="dots">
        <break strength="strong">
    </cmd>

    <cmd name="em">
        <emphasis level = "strong">
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name="emph">
        <emphasis level = "strong">
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name="textit">
        <emphasis level = "strong">
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name="textbf">
        <emphasis level = "strong">
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name = "texttt">
        <emphasis level = "strong">
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name = "cite">
        <emphasis level = "reduced">
            Cited in reference as:
            <break time = "0.3s"/>
            <arg num = "1"/>
        </emphasis>
    </cmd>

    <cmd name = "bibitem">
        <break time = "1s"/>
        Bibliography item is read as:
        <break time = "0.5s"/>
        <arg num = "1">
    </cmd>

    <cmd name = "LaTeX">
        LaTeX
    </cmd>

    <cmd name = "caption">
        Caption:
        <break time = "0.3s"/>
        <arg num = "1"/>
        <break time = "0.5s"/>
    </cmd>

    <!-- This is incase user does \thebibliography -->
    <cmd name = "thebibliography">
        <emphasis level = "strong">
            Reference Section: 
        </emphasis>
    </cmd>

<!-- Latex Environments -->
    <!-- This is incase user does \begin{thebibliography} -->
    <env name = "thebibliography">
        <says>
            <emphasis level = "strong">
                Reference Section: 
                <content/>
            </emphasis>
        </says>
    </env>

    <env name="equation" mathmode="true">
        <says>
            the equation
            <content/>
        </says>
    </env>

    <env name="eqnarray" mathmode="true">
    </env>

    <env name="$" mathmode="true">
    </env>

    <env name="$$" mathmode="true">
    </env>

    <env name="displaymath" mathmode="true"> <!-- Also alias for \[...\] in TexSoup -->
    </env>

    <env name="math" mathmode="true"> <!-- Also alias for \(...\) in TexSoup-->
    </env>

    <env name="itemize">
        <says>
            <content/>
        </says>

        <!-- This might be wrong, because we want the contents to be read with break time infront? -->
        <defines>
            <cmd name="item">
                <break time = "40ms"/>
            </cmd>

        </defines>

        <break time = "medium"/>
    </env>

    <env name="quotation">
        <says>
            &lt;p&gt;
                <content/>
            &lt;/p&gt;
        </says>
    </env>

    <env name="quote">
        <says>
            &lt;p&gt;
                <content/>
            &lt;/p&gt;
        </says>
    </env>

    <env name="center">
        <says>
            &lt;p&gt;
                <content/>
            &lt;/p&gt;
        </says>
    </env>

    <env name="flushleft">
        <says>
            &lt;p&gt;
                <content/>
            &lt;/p&gt;
        </says>
    </env>

    <env name="flushright">
        <says>
            &lt;p&gt;
                <content/>
            &lt;/p&gt;
        </says>
    </env>

    <env name="verse">
        <says>
            &lt;p&gt;
                <content/>
            &lt;/p&gt;
        </says>
    </env>

    <env name="enumerate">
    </env>

    <env name="description">
    </env>

    <env name="list">
    </env>

    <env name="array">
    </env>

    <env name="table">
        <says>
            Begin Table:
            <break time = "0.3s"/>
            <content/>
        </says>
    </env>

    <env name="tabular" readTable = "true">
        <says>
            Table Contents:
            <break time = "40ms"/>
            <content/>
        </says>
    </env>

    <env name="abstract">
    </env>

    <env name="minipage">
    </env>

    <env name="split">
    </env>

    <env name="tabbing">
    </env>

    <env name="titlepage">
    </env>

    <env name="figure">
    </env>

    <env name="picture">
    </env>

    <env name="verbatim">
    </env>
</latex>
