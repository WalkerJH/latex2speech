<?xml version="1.0" encoding="UTF-8"?>

<latex>
    <cmd name="section">
        <prefix>section </prefix>
        <arg say_contents="true"/>
    </cmd>
    <cmd name="dots">
        <say_as>&lt;break strength="strong"/&gt;</say_as>
    </cmd>
    <cmd name="em">
        <prefix>&lt;emphasis level="strong"&gt;</prefix>
        <arg say_contents="true"/>
        <suffix>&lt;/emphasis&gt;</suffix>
    </cmd>
    <cmd name="emph">
        <prefix>&lt;emphasis level="strong"&gt;</prefix>
        <arg say_contents="true"/>
        <suffix>&lt;/emphasis&gt;</suffix>
    </cmd>
    <cmd name="textit">
        <prefix>&lt;emphasis level="strong"&gt;</prefix>
        <arg say_contents="true"/>
        <suffix>&lt;/emphasis&gt;</suffix>
    </cmd>
    <cmd name="textbf">
        <prefix>&lt;emphasis level="strong"&gt;</prefix>
        <arg say_contents="true"/>
        <suffix>&lt;/emphasis&gt;</suffix>
    </cmd>
    <cmd name = "&amp;">
        <prefix>and</prefix>
    </cmd>
    <env name="equation" mathmode="true">
        <prefix>the equation</prefix>
    </env>
    <env name="eqnarray" mathmode="true">
    </env>
    <env name="$" mathmode="true">
    </env>
    <env name="$$" mathmode="true">
    </env>
    <env name="displaymath" mathmode="true"> <!-- Also alias for \[...\] in TexSoup -->
    </env>
    <env name="math" mathmode="true"> <!-- Also alias for \(...\) in TexSoup-->
    </env>
    <env name="itemize">
        <cmd name="item">
            <prefix>&lt;break time="40ms"/&gt;</prefix>
        </cmd>
        <suffix>&lt;break strength="medium"/&gt;</suffix>
    </env>
    <env name="quotation">
        <prefix>&lt;p&gt;</prefix>
        <suffix>&lt;/p&gt;</suffix>
    </env>
    <env name="quote">
        <prefix>&lt;p&gt;</prefix>
        <suffix>&lt;/p&gt;</suffix>
    </env>
    <env name="center">
        <prefix>&lt;p&gt;</prefix>
        <suffix>&lt;/p&gt;</suffix>
    </env>
    <env name="flushleft">
        <prefix>&lt;p&gt;</prefix>
        <suffix>&lt;/p&gt;</suffix>
    </env>
    <env name="flushright">
        <prefix>&lt;p&gt;</prefix>
        <suffix>&lt;/p&gt;</suffix>
    </env>
    <env name="verse">
        <prefix>&lt;p&gt;</prefix>
        <suffix>&lt;/p&gt;</suffix>
    </env>
    <env name="enumerate">
    </env>
    <env name="description">
    </env>
    <env name="list">
    </env>
    <env name="array">
    </env>
    <env name="table">
    </env>
    <env name="tabular">
    </env>
    <env name="abstract">
    </env>
    <env name="minipage">
    </env>
    <env name="split">
    </env>
    <env name="tabbing">
    </env>
    <env name="titlepage">
    </env>
    <env name="figure">
    </env>
    <env name="picture">
    </env>
    <env name="verbatim">
    </env>
</latex>
